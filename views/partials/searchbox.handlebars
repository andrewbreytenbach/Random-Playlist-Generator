<div class="music-search-container">
    <h2>Search Music By Filters</h2>
    <hr>
    <div class="filter-wrapper">
        <button id="artist">Artist</button>
        <input type="text" id="input-one" placeholder="Please enter an artist">
    </div>
    <div class="filter-wrapper">
        <button id="genre">Genre</button>
        <input type="genre" id="input-two" placeholder="Please enter a genre">
    </div>
    <div class="filter-wrapper">
        <button id="decade">Decade</button>
        <input type="decade" id="input-three" placeholder="Please enter a decade">
    </div>
    <div class="filter-wrapper">
        <button id="year">Year</button>
        <input type="year" id="input-four" placeholder="Please enter a year">
    </div>
</div>
